<div class='section contests-all'>
	  <h1 class='text-align-center title-on-page'>Предметные олимпиады</h1>
	  <% @contests.each do |contest| %>
	  	<%= link_to contest_path(contest) do %>
				<div class='col col-33p'>
					<img src='<%= contest.image %>' />
					<h4><%= contest.title %></h4>
				</div>
			<% end %>
		<% end %>
		<div class='clearboth'>
		</div>
</div>